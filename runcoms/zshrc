  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs..#
alias tmux='tmux -2 -CC' 
alias mailcatch='mailcatcher --http-ip=33.33.33.10'
alias tomcat-up='sudo $CATALINA_HOME/bin/startup.sh'
alias tomcat-down='sudo $CATALINA_HOME/bin/shutdown.sh'
alias cmd-debug='export XDEBUG_CONFIG="idekey=PHPStorm"'
alias cmd-debug-off='export XDEBUG_CONFIG=""'
alias keys='eval `keychain --eval --agents ssh id_rsa votasegunpoliticas.pem`'
alias ssh-minecraft='ssh -i ~/.ssh/minecraft-asw.pem ec2-user@ec2-52-31-196-114.eu-west-1.compute.amazonaws.com'

export LC_ALL=en_GB.UTF-8
export ANDROID_HOME=/usr/local/opt/android-sdk

